<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uipeformance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".pre.MainActivity"></activity>
        <activity android:name=".after.MainActivity"/>
        <activity android:name=".pre.HierarchyActivity" />
        <activity android:name=".after.HierarchyActivity" />
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pre.GCActivity" />
        <activity android:name=".after.GCActivity" />
        <activity android:name=".pre.OverdrawActivity" />
        <activity android:name=".after.OverdrawActivity"
            android:theme="@style/AppThemeOver"/>
        <activity android:name=".pre.UIConsumeActivity" />
        <activity android:name=".after.UIConsumeActivity" />
        <activity android:name=".pre.BitmapActivity" />
        <activity android:name=".after.BitmapActivity" />
     </application>

</manifest>